{
  "tab_size": 2,
  "languages": {
    "Python": {
      "language_servers": ["pyright"]
    }
  },
  "lsp": {
    "pyright": {
      "settings": {
        "python.analysis": {
          "diagnosticMode": "workspace",
          "typeCheckingMode": "strict",
          "extraPaths": ["./src", "./run_py"]
        }
      }
    }
  }
}
